{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/Cadet/get/{cadetId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cadetapi",
          //"Host": "host.docker.internal",
          //"Port": "44387"
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/getCadet/{cadetId}",
      "Key": "Cadet"
    },
    {
      "DownstreamPathTemplate": "/api/Cadet/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "cadetapi",
          //"Host": "host.docker.internal",
          //"Port": "44387"
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/Cadet/{everything}",
      "Key": "CadetAll"
    },
    {
      "DownstreamPathTemplate": "/api/courseHistory/getForCadet/{cadetId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "coursehistory",
          //"Host": "host.docker.internal",
          //"Port": "44387"
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/getCadetCourseHistory/{cadetId}",
      "Key": "CourseHistory"
    }
  ],
  "Aggregates": [
    {
      "ReRouteKeys": [
        "Cadet",
        "CourseHistory"
      ],
      "UpstreamPathTemplate": "/CadetAndCourseHistory/{cadetId}"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000/"
   
  }
}